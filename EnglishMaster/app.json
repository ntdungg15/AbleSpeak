{
  "expo": {
    "name": "EnglishMaster",
    "slug": "EnglishMaster",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-dog.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "y",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "permissions": [
        "INTERNET",
        "INTERNET"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-dog.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.longhoanglekim.EnglishMaster"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-dev-client",
      "expo-av"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "554eb83b-80f6-4b2c-91d4-494db0b57fde"
      }
    }
  }
}
